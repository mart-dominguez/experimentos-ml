      <div data-type="elementContainer" style="display:none;">
        <div data-type="team">ISL</div>
      </div>
      <div class="fi-lb__post-userpicture">
        <img src="https://img.fifa.com/image/upload/t_s4/fwc2018-correspondents/17-Petur_Hreirsson_0203_FINAL-V01.jpg" class="" alt="Pétur Hreinsson">
