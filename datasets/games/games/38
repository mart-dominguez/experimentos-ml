    if (DBClickKeyValues != null) {
      DBClickKeyValues = DBClickKeyValues.toString();
      DBClickKeyValues = DBClickKeyValues.split(',');
      for (var i = 0; i != DBClickKeyValues.length; i++) {
        var key = DBClickKeyValues[i].split('=')[0];
