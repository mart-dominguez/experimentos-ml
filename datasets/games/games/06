            <!--[if IE 9]><video style="display: none;"><![endif]-->
            <source srcset="{{{data.ImagePath}}}" media="(min-width: 0)" ></source>
            <!--[if IE 9]></video><![endif]-->
            <img src="{{{data.ImagePath}}}" class="img-responsive" />
          </picture>
          <span class="d3-o-media-object__kind d3-o-media-object-kind--bottom-end">
            <span class="d3-o-icon fi-icon-videos"></span>
          </span>
        </a>
        {{else}}
        <span class="fi-o-media-object__link">
          <picture>
