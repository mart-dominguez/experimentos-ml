    var DBClickKeyValues = document.location.pathname.match(/[^/]*=[^/|^\.]*/gi);
    if (DBClickKeyValues != null) {
      DBClickKeyValues = DBClickKeyValues.toString();
      DBClickKeyValues = DBClickKeyValues.split(',');
      for (var i = 0; i != DBClickKeyValues.length; i++) {
        var key = DBClickKeyValues[i].split('=')[0];
        var value = DBClickKeyValues[i].split('=')[1];
        if (typeof value != 'undefined') {
          if (value.indexOf('-') !== -1) {
            value = value.split('-');
            value = value[value.length - 1];
          }
        } googletag.cmd.push(function () {
          googletag.pubads().setTargeting(key, [value]);
        });
